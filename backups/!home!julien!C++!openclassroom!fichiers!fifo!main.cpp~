#include <iostream>
#include <string>
#include <cerrno>
#include <fstream>


#include <sys/types.h>
#include <sys/stat.h>
#include <unistd.h> 


using namespace std;



int main(){
  
  fstream in;
  fstream read;
  const char* test;
  struct stat st;
  
  if (stat(test, &st) != 0){
    mkfifo (test, 0666);
  }
  
  string line;
  read.open(test, fstream::in);
  while (1){
    read >> line;
    cout << line <<endl;
  }

  return 0;
  
}
